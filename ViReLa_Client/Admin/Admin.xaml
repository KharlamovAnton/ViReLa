<Window x:Class="ViReLa_Client.Admin.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViReLa_Client.Admin"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800" Closed="Window_Closed">
    <Grid>
        <TreeView HorizontalAlignment="Left" Height="300" Margin="10,110,0,0" VerticalAlignment="Top" Width="157" Background="#FF079ED8" Name="twTree" SelectedItemChanged="twTree_SelectedItemChanged"/>
        <Image HorizontalAlignment="Left" Height="95" Margin="10,10,0,0" VerticalAlignment="Top" Width="157" Source="logo.png"/>
        <Grid HorizontalAlignment="Left" Height="300" Margin="176,110,0,0" VerticalAlignment="Top" Width="608" Name="gFree">
        </Grid>
        <Button Content="Обновить" HorizontalAlignment="Left" Margin="172,86,0,0" VerticalAlignment="Top" Width="143" Click="Button_Click"/>
        <Button Content="Удалить" HorizontalAlignment="Left" Margin="320,87,0,0" VerticalAlignment="Top" Width="159" RenderTransformOrigin="0.5,0.5" Click="Button_Click_1">
        </Button>
    </Grid>
</Window>
